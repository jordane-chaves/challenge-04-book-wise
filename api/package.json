  "type": "module",
  "main": "index.js",
  "scripts": {
    "start:dev": "node --env-file .env --watch src/infra/server.ts",
    "prestart": "drizzle-kit migrate && node src/infra/database/drizzle/seed/seed.ts",
    "start": "node src/infra/server.ts",
    "lint": "biome check",
    "format": "biome format --write",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:studio": "drizzle-kit studio",
    "db:seed": "node --env-file .env src/infra/database/drizzle/seed/seed.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "db:reset-and-migrate": "node --env-file .env --env-file .env.test test/utils/reset-database.ts && dotenv -e .env.test drizzle-kit migrate",
    "pretest:e2e": "npm run db:reset-and-migrate",
    "test:e2e": "dotenv -e .env -e .env.test --override -- vitest run --config ./vitest.config.e2e.ts",
    "pretest:e2e:watch": "npm run db:reset-and-migrate",
    "test:e2e:watch": "dotenv -e .env -e .env.test --override -- vitest --config ./vitest.config.e2e.ts"
  },
